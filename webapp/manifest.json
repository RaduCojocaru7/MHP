{
  "_version": "1.65.0",
  "sap.app": {
    "id": "fbtool",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": { "version": "0.0.1" },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.3",
      "toolsId": "07a10b4b-f6af-4ccd-9c88-0dac23e81a2d"
    },
    "dataSources": {
      "ZARC_FEEDBACK_TOOL_SRV": {
        "uri": "/sap/opu/odata/sap/ZARC_FEEDBACK_TOOL_SRV/",
        "type": "OData",
        "settings": { "odataVersion": "2.0" }
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": { "icon": "", "favIcon": "", "phone": "", "phone@2": "", "tablet": "", "tablet@2": "" },
    "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": { "viewName": "fbtool.view.App", "type": "XML", "id": "App", "async": true },
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": { "sap.m": {}, "sap.ui.core": {} }
    },
    "contentDensities": { "compact": true, "cozy": true },

    "models": {
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "ZARC_FEEDBACK_TOOL_SRV",
        "preload": true,
        "settings": {
          "json": true,
          "loadMetadataAsync": true,
          "defaultBindingMode": "OneWay",
          "useBatch": false,
          "defaultOperationMode": "Server",
          "defaultCountMode": "Request",
          "refreshAfterChange": false,
          "preliminaryContext": true,
          "skipMetadataAnnotationParsing": true
        }
      },
      "mainService": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "ZARC_FEEDBACK_TOOL_SRV",
        "settings": {
          "json": true,
          "loadMetadataAsync": true,
          "defaultBindingMode": "OneWay",
          "useBatch": false,
          "defaultOperationMode": "Server",
          "defaultCountMode": "Request",
          "refreshAfterChange": false,
          "preliminaryContext": true,
          "skipMetadataAnnotationParsing": true
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": { "bundleName": "fbtool.i18n.i18n" }
      }
    },

    "resources": {
      "css": [{ "uri": "css/style.css" }]
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "fbtool.view",
        "async": true
      },
      "routes": [
        { "name": "Login", "pattern": "", "target": ["TargetLogin"] },
        { "name": "ForgotPassword", "pattern": "ForgotPassword", "target": ["TargetForgotPassword"] },
        { "name": "UserDashboard", "pattern": "UserDashboard", "target": ["TargetUserDashboard"] },
        { "name": "ManagerDashboard", "pattern": "ManagerDashboard", "target": ["TargetManagerDashboard"] },
        { "name": "HRDashboard", "pattern": "HRDashboard", "target": ["TargetHRDashboard"]},
        { "name": "fb360", "pattern": "fb360", "target": ["fb360"] },
        { "name": "FeedbackRequest", "pattern": "FeedbackRequest", "target": ["TargetFeedbackRequest"] },
        { "name": "PEGRequest", "pattern": "PEGRequest", "target": ["TargetPEGRequest"] },
        { "name": "PEGList", "pattern": "PEGList", "target": ["TargetPEGList"] },
        { "name": "FeedbackList", "pattern": "FeedbackList", "target": ["TargetFeedbackList"] },
        { "name": "ProfileInfo", "pattern": "ProfileInfo", "target": ["TargetProfileInfo"] },
        { "pattern": "MyTeam", "name": "MyTeam", "target": ["MyTeam"] },
        { "name": "EmployeeList", "pattern": "EmployeeList", "target": ["TargetEmployeeList"]}
      ],
      "targets": {
        "TargetLogin": { 
          "id": "Login", 
          "name": "Login" 
        },
        "TargetForgotPassword": { 
          "id": "ForgotPassword", 
          "name": "ForgotPassword" 
        },
        "TargetUserDashboard": { 
          "id": "UserDashboard", 
          "name": "UserDashboard" 
        },
        "TargetManagerDashboard": { 
          "id": "ManagerDashboard", 
          "name": "ManagerDashboard" 
        },
        "TargetHRDashboard": { 
          "id": "HRDashboard", 
          "name": "HRDashboard" 
        },
        "TargetEmployeeList": {
          "type": "View",
          "name": "EmployeeList",
          "viewType": "XML",
          "id": "EmployeeList",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "fb360": {
          "type": "View",
          "name": "360FB",
          "viewType": "XML",
          "id": "fb360",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "TargetProfileInfo": {
          "type": "View",
          "name": "ProfileInfo",
          "viewType": "XML",
          "id": "ProfileInfo",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "TargetPEGRequest": {
          "type": "View",
          "name": "PEGRequest",
          "viewType": "XML",
          "id": "PEGRequest",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "TargetPEGList": {
          "type": "View",
          "name": "PEGList",
          "viewType": "XML",
          "id": "PEGList",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "TargetFeedbackList": {
          "type": "View",
          "name": "FeedbackList",
          "viewType": "XML",
          "id": "FeedbackList",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "MyTeam": {
          "type": "View",
          "name": "MyTeam",
          "viewType": "XML",
          "id": "MyTeam",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        },
        "TargetFeedbackRequest": {
          "type": "View",
          "name": "FeedbackRequest",
          "viewType": "XML",
          "id": "FeedbackRequest",
          "controlAggregation": "pages",
          "controlId": "app",
          "transition": "slide"
        }
      }
    }
  }
}